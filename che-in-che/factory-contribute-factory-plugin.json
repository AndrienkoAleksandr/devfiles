{
  "v": "4.0",
  "name": "contribute_factory",
  "workspace": {
    "defaultEnv": "default",
    "environments": {
      "default": {
        "machines": {
          "ws/git": {
            "attributes": {},
            "servers": {},
            "volumes": {
              "projects": {
                "path": "/projects"
              }
            },
            "installers": [],
            "env": {}
          },
          "ws/dev": {
            "attributes": {},
            "servers": {},
            "volumes": {
              "projects": {
                "path": "/projects"
              }
            },
            "installers": [],
            "env": {}
          }
        },
        "recipe": {
          "type": "kubernetes",
          "content": "kind: List\nitems:\n - \n  apiVersion: v1\n  kind: Pod\n  metadata:\n   name: ws\n  spec:\n   containers:\n    - \n     image: wsskeleton/che-plugin-dev-tooling\n     name: dev\n     resources:\n      limits:\n       memory: 2048Mi\n    - \n     image: sunix/git-devtools\n     name: git\n     resources:\n      limits:\n       memory: 921.6000003814697Mi\n",
          "contentType": "application/x-yaml"
        }
      }
    },
    "projects": [
      {
        "links": [],
        "name": "che-theia-factory-extension",
        "attributes": {},
        "type": "blank",
        "source": {
          "location": "https://github.com/eclipse/che-theia-factory-extension.git",
          "type": "git",
          "parameters": {
            "branch": "WIP-test"
          }
        },
        "path": "/che-theia-factory-extension",
        "description": "",
        "mixins": [],
        "problems": []
      }
    ],
    "name": "contribute-to-che-theia-factory-plugin",
    "attributes": {
      "editor": "org.eclipse.che.editor.theia:1.0.0",
      "persistVolumes": "false",
      "plugins": "che-machine-exec-plugin:0.0.1",
      "che.server.secure_exposer": "jwtproxy"
    },
    "commands": [
      {
        "commandLine": "echo ${CHE_OSO_CLUSTER//api/console}",
        "name": "Get OpenShift Console URL",
        "attributes": {},
        "type": "custom"
      },
      {
        "commandLine": "rm /default-theia-plugins/eclipse_che_theia_factory_plugin.theia || true && mkdir -p /projects/theiadev_projects && export CHE_PROJECTS_ROOT=/projects/theiadev_projects && export HOSTED_PLUGIN=/projects/che-theia-factory-extension/ && cd && node src-gen/backend/main.js /projects/theiadev_projects --hostname=0.0.0.0 --port=3030",
        "name": "start",
        "attributes": {
          "previewUrl": "${server.theia-dev}",
          "goal": "Build",
          "machineName": "ws/theia-ide"
        },
        "type": "custom"
      },
      {
        "commandLine": "rm ~/.yarnrc || true && cd /projects/che-theia-factory-extension/ && yarn",
        "name": "yarn build",
        "attributes": {
          "previewUrl": "",
          "goal": "Build",
          "machineName": "ws/dev"
        },
        "type": "custom"
      },
      {
        "commandLine": "rm ~/.yarnrc|| true && cd /projects/che-theia-factory-extension/ && yarn && yarn test-watch",
        "name": "yarn build test-watch",
        "attributes": {
          "previewUrl": "",
          "goal": "Build",
          "machineName": "ws/dev"
        },
        "type": "custom"
      }
    ],
    "links": []
  },
  "ide": {
    "onProjectsLoaded": {
      "actions": [
        {
          "properties": {
            "name": "yarn build test-watch"
          },
          "id": "runCommand"
        }
      ]
    }
  }
}